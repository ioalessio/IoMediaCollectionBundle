<div class="row-fluid">
    <div class="plupload" id="{{ form.vars['id'] }}" 
         data-upload="{{ path('io_media_upload') }}" 
         data-prototype="{{ form_widget(form.vars['prototype'])|e }}" 
         data-picker="{{ form.vars['id'] }}_pickfiles"
         data-field="{{ form.vars['media_field'] }}"
    >{{ form_widget(form) }}
    </div>
    <div class="clearfix"></div>
    <div>
        {{ form_rest(form) }}  
        <ul id="{{ form.vars['id'] }}_uploader_list" class="uploader_list nav nav-list"></ul>
        <button id="{{ form.vars['id'] }}_pickfiles" class="btn btn-primary" type="button">Select files</button> 
        <button id="{{ form.vars['id'] }}_uploadfiles" class="btn btn-primary" type="button">Upload files</button> 
    </div>
</div>